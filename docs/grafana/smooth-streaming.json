{
  "dashboard": {
    "title": "Auralis Smooth Audio Streaming (US1)",
    "description": "Monitor real-time audio streaming performance: chunk delivery jitter, buffer health, and latency metrics (Task T036 [US1])",
    "tags": ["auralis", "streaming", "performance", "us1"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Chunk Delivery Jitter (P95/P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P99 Jitter - {{client_id}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P95 Jitter - {{client_id}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P50 Jitter - {{client_id}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Jitter", "show": true},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [50], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Chunk Delivery Jitter",
          "message": "P99 chunk delivery jitter exceeded 50ms threshold. Audio playback may experience glitches."
        },
        "description": "Chunk delivery timing jitter (deviation from expected 100ms interval). Lower is better. >50ms may cause audible glitches."
      },
      {
        "id": 2,
        "title": "Buffer Depth Per Client",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "auralis_buffer_depth_ms",
            "legendFormat": "{{client_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Buffer Depth", "show": true, "min": 0, "max": 2000},
          {"show": false}
        ],
        "seriesOverrides": [],
        "description": "Ring buffer depth in milliseconds for each connected client. Target: 300-500ms for smooth playback."
      },
      {
        "id": 3,
        "title": "Synthesis Latency (P95/P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(auralis_synthesis_latency_seconds_bucket[5m])) * 1000",
            "legendFormat": "P99 Synthesis Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(auralis_synthesis_latency_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95 Synthesis Latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency", "show": true},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [100], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Synthesis Latency",
          "message": "Synthesis latency exceeded 100ms real-time constraint. Buffer may underrun."
        },
        "description": "Audio synthesis processing latency. Must stay <100ms for real-time performance."
      },
      {
        "id": 4,
        "title": "Phrase Generation Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "auralis_phrase_generation_rate_hz",
            "legendFormat": "Phrases/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Phrases/sec", "show": true, "min": 0},
          {"show": false}
        ],
        "description": "Musical phrase generation throughput. Indicates synthesis pipeline health."
      },
      {
        "id": 5,
        "title": "Active WebSocket Connections",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "auralis_active_connections",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        },
        "description": "Number of active streaming clients connected via WebSocket."
      },
      {
        "id": 6,
        "title": "Chunk Delivery Jitter Heatmap",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "rate(auralis_chunk_delivery_jitter_ms_bucket[1m])",
            "legendFormat": "{{le}}",
            "refId": "A",
            "format": "heatmap"
          }
        ],
        "dataFormat": "tsbuckets",
        "yAxis": {
          "format": "ms",
          "decimals": 0,
          "logBase": 1
        },
        "cards": {
          "cardPadding": 2,
          "cardRound": 2
        },
        "color": {
          "mode": "spectrum",
          "cardColor": "#b4ff00",
          "colorScale": "sqrt",
          "exponent": 0.5,
          "colorScheme": "interpolateSpectral"
        },
        "description": "Heatmap visualization of chunk delivery jitter distribution over time. Darker areas indicate more frequent jitter values."
      },
      {
        "id": 7,
        "title": "Total Synthesis Operations",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "auralis_synthesis_total",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "description": "Cumulative count of synthesis operations completed since server start."
      },
      {
        "id": 8,
        "title": "Jitter Distribution (Last 5m)",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.90, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P90",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.75, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P75",
            "refId": "D"
          },
          {
            "expr": "histogram_quantile(0.50, rate(auralis_chunk_delivery_jitter_ms_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "E"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 20, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        },
        "description": "Percentile distribution of chunk delivery jitter over the last 5 minutes."
      },
      {
        "id": 9,
        "title": "Buffer Health Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "auralis_buffer_depth_ms",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "client_id": "Client ID",
                "Value": "Buffer Depth (ms)"
              }
            }
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 200, "color": "yellow"},
                {"value": 300, "color": "green"},
                {"value": 800, "color": "yellow"},
                {"value": 1200, "color": "orange"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Buffer Depth (ms)"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            }
          ]
        },
        "description": "Per-client buffer health with color-coded thresholds. Green = optimal (300-800ms), Yellow = acceptable, Red = critical."
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\"High.*\"}",
          "iconColor": "red",
          "name": "Alerts",
          "tagKeys": "alertname",
          "textFormat": "{{alertname}}: {{message}}",
          "titleFormat": "Alert"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 1,
    "links": [],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  }
}
